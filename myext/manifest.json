{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png"
      },
      "default_popup": "popup.html",
      "default_title": "mytest_default_title"
   },
   "content_scripts": [ {
      "css": [ "style/content.css" ],
      "js": [ "js/content.js" ],
      "matches": ["*://*.taobao.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "mytest_description",
   "homepage_url": "https://www.baidu.com",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "mytest_name",
   "permissions": [ "tabs", "storage", "*://*/*" ],
   "short_name": "mytest_short_name",
   "update_url": "http://upext.chrome.360.cn/intf.php?method=ExtUpdate.query",
   "version": "1.0.0",
   "web_accessible_resources": [ "style/*", "js/*", "images/*" ]
}
